{
  "name": "multi-cuke",
  "version": "0.8.4",
  "description": "Forked Cucumber-js for parallel testing",
  "repository": {
    "type": "git",
    "url": "https://github.com/midniteio/multi-cuke.git"
  },
  "keywords": [
    "parallel",
    "test",
    "cucumber",
    "cucumber-js",
    "node",
    "npm",
    "javascript"
  ],
  "main": "distribution/main.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir distribution",
    "prepublish": "npm run build",
    "lint": "eslint src test",
    "unit-test": "mocha --compilers js:babel-register",
    "test": "npm run build && npm run lint --silent && npm run unit-test --silent"
  },
  "dependencies": {
    "bluebird": "^3.3.1",
    "commander": "^2.9.0",
    "cucumber": "^1.0.0",
    "fs-extra": "^0.26.5",
    "gherkin": "^3.2.0",
    "glob": "^7.0.0",
    "lodash": "^4.5.0",
    "pretty-ms": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "eslint": "2.2.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  },
  "author": "Ian Fleming <ian@midnite.io>",
  "license": "ISC"
}
